name=doomgeneric
from_source=doomgeneric
revision=1
hostdeps="gcc automake pkg-config"
deps="mlibc"

build() {
	cp -rp "${source_dir}"/doomgeneric ./
	cd doomgeneric
	make clean
	make -j${parallelism}
}

package() {
	cd doomgeneric
	mkdir -p "${dest_dir}/usr/bin"
	cp -p ./doomgeneric "${dest_dir}/usr/bin/"
}
